{
  "expo": {
    "name": "Rigor",
    "slug": "Rigor",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/RigorLogo.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/RigorLogo.jpg",
        "backgroundColor": "#ffffff"
      },
      "googleMaps": {
        "apiKey": "AIzaSyCiS88RSVg6G4GkWS9S-4ptWUxSxxnY_nc" 
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/truck.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
